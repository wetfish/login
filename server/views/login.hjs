{{> head}}
    <h1>Login</h1>

    <form method="post" action="/login">
        <div>
            <label>Username</label>
            <input name="username">
        </div>

        <div>
            <label>Password</label>
            <input name="password" type="password">
        </div>

        <div>
            <input type="submit" value="Submit">
        </div>

    </form>

    Don't have an account? <a href="/register">Register!</a>
{{> foot}}
